<script type="application/ld+json">
{
	"@context": "https://schema.org",
	"@type": "FAQPage",
	"mainEntity": [
		{% for item in faq %}
		{
			"@type": "Question",
			"name": {{ item.question | jsonify }},
			"acceptedAnswer": {
				"@type": "Answer",
				"text": {{ item.answer | jsonify }}
			}
		}{% if not forloop.last %},{% endif %}
		{% endfor %}
	]
}
</script>
