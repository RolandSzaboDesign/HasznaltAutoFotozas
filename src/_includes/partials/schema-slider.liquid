<script type="application/ld+json">
{
	"@context": "https://schema.org",
	"@type": "ItemList",
	"itemListElement": [
		{%- for item in cars %}
		{
			"@type": "ListItem",
			"position": {{ forloop.index }},
			"item": {
				"@type": "ImageObject",
				"name": "{{ item.car }} – profi fotó",
				"caption": "{{ item.afterAlt }}: {{ item.car }}",
				"contentUrl": "{{ item.after }}",
				"representativeOfPage": true,
				"thumbnail": {
					"@type": "ImageObject",
					"url": "{{ item.after }}"
				}
			}
		}{% unless forloop.last %},{% endunless %}
		{%- endfor %}
	]
}
</script>
